<LinearLayout
    android:orientation="vertical"
    android:id="@+id/activity_start"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StartActivity">


    <ImageView
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/imageView"
        android:background="#ff171665" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_row="0"
                android:layout_column="0"
                android:layout_gravity="fill"
                android:id="@+id/textView"
                android:background="#ff58ffe1"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1" />

            <TextView
                android:layout_row="0"
                android:layout_column="1"
                android:layout_gravity="fill"
                android:id="@+id/textView1"
                android:background="#ffff1a2f"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1" />

            <TextView
                android:layout_row="1"
                android:layout_column="0"
                android:layout_gravity="fill"
                android:id="@+id/textView2"
                android:background="#ffa638ff"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1" />

            <TextView
                android:layout_row="1"
                android:layout_column="1"
                android:layout_gravity="fill"
                android:id="@+id/textView3"
                android:background="#ff786eff"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1" />

            <TextView
                android:layout_row="0"
                android:layout_column="2"
                android:layout_gravity="fill"
                android:id="@+id/textView4"
                android:background="#ffa638ff"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1" />

            <TextView
                android:layout_row="1"
                android:layout_column="2"
                android:layout_gravity="fill"
                android:id="@+id/textView5"
                android:background="#ffffc454"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1" />

            <TextView
                android:layout_row="0"
                android:layout_column="3"
                android:layout_gravity="fill"
                android:id="@+id/textView6"
                android:background="#ff3aafff"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1" />

            <TextView
                android:layout_row="1"
                android:layout_column="3"
                android:layout_gravity="fill"
                android:id="@+id/textView7"
                android:background="#ff786eff"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1" />
        </GridLayout>

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_row="0"
                android:layout_column="0"
                android:layout_gravity="fill"
                android:id="@+id/textView8"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:backgroundTint="#af000000" />

            <TextView
                android:layout_row="0"
                android:layout_column="1"
                android:layout_gravity="fill"
                android:id="@+id/textView9"
                android:background="#ffff1a2f"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1" />

            <TextView
                android:gravity="center"
                android:layout_row="1"
                android:layout_column="0"
                android:layout_gravity="fill"
                android:id="@+id/textView10"
                android:background="#ffa638ff"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:backgroundTint="#af000000" />

            <TextView
                android:layout_row="1"
                android:layout_column="1"
                android:layout_gravity="fill"
                android:id="@+id/textView11"
                android:background="#ff786eff"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:backgroundTint="#af000000" />

            <TextView
                android:layout_row="0"
                android:layout_column="2"
                android:layout_gravity="fill"
                android:id="@+id/textView12"
                android:background="#ffa638ff"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1" />

            <TextView
                android:layout_row="1"
                android:layout_column="2"
                android:layout_gravity="fill"
                android:id="@+id/textView13"
                android:background="#ffffc454"
